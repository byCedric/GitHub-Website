{
	"version": 2,
	"name": "github-website",
	"builds": [
		{ "src": "package.json", "use": "@now/static-build" }
	],
	"routes": [
		{ "src": "^/static/(.*)", "dest": "/static/$1" },
		{ "src": ".*", "dest": "/index.html" }
	],
	"build": {
		"env": {
			"NODE_PATH": "./",
			"REACT_APP_GITHUB_USERNAME": "byCedric"
		}
	}
}
